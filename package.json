	"scripts": {
		"dev": "UV_THREADPOOL_SIZE=8 nodemon --config nodemon.json",
		"dev:admin": "cd admin && yarn dev",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "UV_THREADPOOL_SIZE=8 nest start",
		"start:debug": "UV_THREADPOOL_SIZE=8 nest start --debug --watch",
